{
    "name": "ssl_log",
    "version": "0.0.1",
    "description": "Frida agent for logging SSL traffic on Android",
    "private": true,
    "main": "agent/ssl_log.ts",
    "scripts": {
        "prepare": "npm run build",
        "build": "frida-compile agent/ssl_log.ts -o _ssl_log.js",
        "watch": "frida-compile agent/ssl_log.ts -o _ssl_log.js -w"
    },
    "devDependencies": {
        "@types/frida-gum": "^16.0.0",
        "@types/node": "^14.0.14",
        "frida-compile": "^9.3.0"
    }
}